@model langfvn.Models.Place

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h3>QUẢN LÝ KHU VỰC</h3>
<div>
    <i class="fas fa-undo-alt"></i>
    <a href="/admin/places">Quay về danh sách khu vực</a>
</div>
<div class="card shadow mb-4 mt-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-dark">CHI TIẾT KHU VỰC #@Model.PlaceID</h6>
    </div>
    <div class="card-body">
        <div class="form-group row align-items-center mb-0">
            <label for="staticEmail" class="col-sm-2 col-form-label font-weight-bold">ID</label>
            <div class="col-sm-10">
                @Html.DisplayFor(model => model.PlaceID)
            </div>
        </div>
        <div class="form-group row align-items-center mb-0">
            <label for="staticEmail" class="col-sm-2 col-form-label font-weight-bold">Khu vực</label>
            <div class="col-sm-10">
                @Html.DisplayFor(model => model.PlaceName)
            </div>
        </div>
    </div>
</div>
